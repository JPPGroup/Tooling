<!-- Jpp.Common.Tooling.Signing.targets -->
<Project> 
  <Target Name="RunProtection" AfterTargets="SignAssembly" Condition="'$(Configuration)' == 'Release' ">
    <Exec Command="powershell.exe –NonInteractive –ExecutionPolicy Unrestricted –Command &quot;&amp; { $(MSBuildThisFileDirectory)..\tools\Protect.ps1 -ProjectPath:$(MSBuildProjectDirectory) }&quot;"/>
  </Target>
</Project>